<?xml version="1.0" encoding="utf-8"?>
<Report>
  <!--保存报告是否弹出MessageBox提示框-->
  <IsShowMessageBox>true</IsShowMessageBox>
  <!--系统模板与自定义模板中检查所见与检查结论文本框是否隐藏true为隐藏-->
  <IsTempLateSeeAndResultVisible>true</IsTempLateSeeAndResultVisible>
  <!--配置报告编辑选项页中，哪些选项页显示，比如：系统模板|自定义模板|词库|检查测值-->
  <WhethervisibleTabOptions>系统模板|词库|检查测值</WhethervisibleTabOptions>
  <!--是否开启自动更新-->
  <IsOpenPacsUpdate>false</IsOpenPacsUpdate>
  <!--是否开启 每隔指定时间自动保存报告  true 开启  false 不开启-->
  <OpenAutoSaveReport>false</OpenAutoSaveReport>
  <!--每隔指定时间自动保存报告  单位：毫秒(1000=1秒)-->
  <AutoSaveReportInterval>3000</AutoSaveReportInterval>
  <!--本机器是否允许打印A4纸张报告，如果为false，单击打印，则会由其他电脑打印，这个是适用于远程会诊系统的-->
  <OpenRemotePrintReport>true</OpenRemotePrintReport>
  <!--关于打印报告的时间间隔 1000=1秒-->
  <RemotePrintThreadSpacing>20000</RemotePrintThreadSpacing>
  <seeText>检查所见</seeText>
  <resultText>检查结论</resultText>
  <AdviseText></AdviseText>
  <SeeAndResultHeight>
    <MRSeeHeight>490</MRSeeHeight>
    <MRResultHeight>260</MRResultHeight>
    <MRISeeHeight>490</MRISeeHeight>
    <MRIResultHeight>260</MRIResultHeight>
    <DRSeeHeight>220</DRSeeHeight>
    <DRResultHeight>260</DRResultHeight>
    <DXSeeHeight>490</DXSeeHeight>
    <DXResultHeight>260</DXResultHeight>
    <CRSeeHeight>490</CRSeeHeight>
    <CRResultHeight>260</CRResultHeight>
    <CTSeeHeight>220</CTSeeHeight>
    <CTResultHeight>220</CTResultHeight>
    <USWSSeeHeight>490</USWSSeeHeight>
    <USWSResultHeight>260</USWSResultHeight>
    <彩超SeeHeight>490</彩超SeeHeight>
    <彩超ResultHeight>260</彩超ResultHeight>
    <黑白超SeeHeight>220</黑白超SeeHeight>
    <黑白超ResultHeight>220</黑白超ResultHeight>
    <OTSeeHeight>490</OTSeeHeight>
    <OTResultHeight>260</OTResultHeight>
    <RFSeeHeight>490</RFSeeHeight>
    <RFResultHeight>260</RFResultHeight>
    <胃镜SeeHeight>490</胃镜SeeHeight>
    <胃镜ResultHeight>260</胃镜ResultHeight>
    <肠镜SeeHeight>220</肠镜SeeHeight>
    <肠镜ResultHeight>220</肠镜ResultHeight>
    <十二指肠镜SeeHeight>220</十二指肠镜SeeHeight>
    <十二指肠镜ResultHeight>220</十二指肠镜ResultHeight>
    <支气管镜SeeHeight>220</支气管镜SeeHeight>
    <支气管镜ResultHeight>220</支气管镜ResultHeight>
    <宫腔镜SeeHeight>220</宫腔镜SeeHeight>
    <宫腔镜ResultHeight>220</宫腔镜ResultHeight>
    <关节镜SeeHeight>220</关节镜SeeHeight>
    <关节镜ResultHeight>220</关节镜ResultHeight>
    <阴道镜SeeHeight>220</阴道镜SeeHeight>
    <阴道镜ResultHeight>220</阴道镜ResultHeight>
    <胆道镜SeeHeight>220</胆道镜SeeHeight>
    <胆道镜ResultHeight>220</胆道镜ResultHeight>
    <耳鼻咽喉镜SeeHeight>220</耳鼻咽喉镜SeeHeight>
    <耳鼻咽喉镜ResultHeight>220</耳鼻咽喉镜ResultHeight>
  </SeeAndResultHeight>
  <!--脚注对其方式(left center  right)-->
  <FontNoteOnTheWay>center</FontNoteOnTheWay>
  <AldSet>
    <!--是否开启报告预览截图功能 true:开启-->
    <OpenTheCcreenshot>true</OpenTheCcreenshot>
    <!--根据密码，判断报告样式设置按钮是否启用启用，如果为空，则不需要密码-->
    <LockAldReport>123</LockAldReport>
    <!--报告中图片是否需要下面的标注 true：需要-->
    <WhetherSpecialType>true</WhetherSpecialType>
    <!--检查所见与检查结论字体大小-->
    <SeeAndResultFontSize>12</SeeAndResultFontSize>
    <!--纸质报告单中，检查所见与检查结论字体大小报告控件中的字体大小的字号。如果大2个字号，则+2-->
    <ReportPrintAddFontSize>+0</ReportPrintAddFontSize>
    <!--是否开启  设置工作站一行最多显示的图像数量 开启true-->
    <IsOpenMaxShowImageCount>false</IsOpenMaxShowImageCount>
    <!--设置工作站一行最多显示的图像数量-->
    <ReportMaxShowImageCount>3</ReportMaxShowImageCount>   
    <!--设置报告图像数量对应报告单中每行显示的的图像数量格式：两张图一行两张，3张图一行三张 1:1|2:2|3:3|4:2|5:3|6:3|7:4|8:4|9:4-->
    <ImageShowNumber>1:1|2:2|3:3|4:3|5:3|6:3|7:4|8:4|9:4</ImageShowNumber>
    <!--是否加载医生签名的图片 添加:true-->
    <IsShowSignature>false</IsShowSignature>
    <!---是否加载脚注和急诊脚注true为加载  用|符号分割  true 加载-->
    <WhetherAddFootnote>false|false</WhetherAddFootnote>
  </AldSet>
</Report>