<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--采图悬浮图像显示位置设置-->
  <SuspensionPicX>906</SuspensionPicX>
  <SuspensionPicY>311</SuspensionPicY>
  <!--是否开启打印照片功能-->
  <IsOpenImgPtint>true</IsOpenImgPtint>
  <HospitalName>中国测试第一人名医院</HospitalName>
  <VideoPreviewWidth>720</VideoPreviewWidth>
  <VideoPreviewHeight>576</VideoPreviewHeight>
  <!--采集卡的视频通道-->
  <VideoChannelNum>0</VideoChannelNum>
  <!--录制视频时，视频的帧率。-->
  <VideoFrameRate>25</VideoFrameRate>
  <!--配置：勾选报告图片时，选择的图片是否自动跳到最前面。自动跳:true,不跳：false-->
  <JumpSelectImage>false</JumpSelectImage>
  <!--配置：配置放大报告图片是鼠标悬浮还是单击  true:悬浮，false:单击-->
  <OpenImageIsMouseMove>true</OpenImageIsMouseMove>
  <USB>1</USB>
  <!--记录脚踏板的彩图模式，前台采图：1,后台：其他-->
  <USBModel>1</USBModel>
  <ShortCutKeys>Ctrl+Alt+P</ShortCutKeys>
  <!--设定哪个按钮是采图、那个按钮时录像（四个按钮分别是：1、2、3、4）-->
  <!--是否开启用采集器采图、注意如果这里设为true，那么USB属性不能=0-->
  <WhetherToOpenCollector>false</WhetherToOpenCollector>
  <!--按钮每次按下停顿时间(单位：毫秒)-->
  <CollectorSleepTimes>500</CollectorSleepTimes>
  <!--采图按钮-->
  <SetMiningPlanButton>1</SetMiningPlanButton>
  <!--录像按钮-->
  <SetVideoButton>3</SetVideoButton>
  <!--弹出视频窗体按钮-->
  <SetShowForm>4</SetShowForm>
  <!--连接视频按钮-->
  <SetConnectTheVideo>
  </SetConnectTheVideo>
  <!--后台采集图像-->
  <SetBackgroundStockPhotography>2</SetBackgroundStockPhotography>
  <!--工作站是否开启默认正常图片(第一个代表是否开启该功能，第二个代表多张图片是否默认勾选(格式:true|true))-->
  <DefaultAddPictures>true|true</DefaultAddPictures>
  <!--后台采集录像-->
  <SetBackgroundVideoLibrary>4</SetBackgroundVideoLibrary>
  <!--后台图片路径-->
  <BackgroundImagePath>D:\PACSData\BackgroundImage</BackgroundImagePath>
  <!--后台视频路径-->
  <BackgroundVideoPath>D:\PACSData\BackgroundImage</BackgroundVideoPath>
  <!--前台采集图像声音频率、后台采图声音频率、采集前台视频频率   用|符号分隔-->
  <SoundFreq>2000|3000|4000</SoundFreq>
  <GraphicCollectionSet>
    <node key="shape" value="circle">
    </node>
    <node key="fill" value="0">
    </node>
    <node key="x1" value="48">
    </node>
    <node key="y1" value="62">
    </node>
    <node key="x2" value="664">
    </node>
    <node key="y2" value="452">
    </node>
    <node key="PicSavePath" value="\\192.168.1.88\PACSData\Image">
    </node>
    <node key="Com" value="1">
    </node>
    <node key="BaudRate" value="19200">
    </node>
  </GraphicCollectionSet>
  <CompressorName>Xvid MPEG-4 Codec</CompressorName>
  <ManualOrDefault>True</ManualOrDefault>
  <VideoSavePath>D:\PACSData\video</VideoSavePath>
  <CaptureTime>999</CaptureTime>
  <imageCount>100</imageCount>
  <imageColumn>6</imageColumn>
  <RowCount>6</RowCount>
  <CT>CT_report.ald</CT>
  <CR>CR_report.ald</CR>
  <DX>DX_report.ald</DX>
  <DR>
  </DR>
  <MR>
  </MR>
  <MRI>
  </MRI>
  <RF>
  </RF>
  <XA>
  </XA>
  <OT>
  </OT>
  <!--配置本机是否安装高配仪 true:有-->
  <GaoPaiyi>true</GaoPaiyi>
  <!--查看检查申请单扫描图片：扫图方式【0：vedio.dll方式 - lxd  1：Twain方式 - lxd  2：无界面扫图 - pzj  3：带预览界面扫图 - pzj】-->
  <GaoPaiyi>true</GaoPaiyi>
  <scaneQuipment>2</scaneQuipment>
  <!--检查申请单扫描图片的软件(exe)的路径-->
  <OpenImagePath>C:\Program Files\ACDSee5\ACDSee5.exe</OpenImagePath>
  <GrabberGray8Image>1</GrabberGray8Image>
  <!--DSHOW、SDK3000API、OKAPI32-->
  <VideoCaptureMode>ZASX</VideoCaptureMode>
  <ShowVideoToScreen>1</ShowVideoToScreen>
  <!--鼠标悬浮在图片是否最大化显示-->
  <ImageIsZoom>false</ImageIsZoom>
  <!--采集到的图像类型：参考(大小写区分):.jpg,.bmp,.jpeg,.gif,.png,.ico,.tif-->
  <SetImageType>.jpg</SetImageType>
  <!--颜色空间(压缩)-->
  <MediaSubType>RGB32</MediaSubType>
  <!--选择默认的数据源（1代表第一个数据源，2代表第二个数据源，空代表第1个数据源）-->
  <DefaultOutputDevice>1</DefaultOutputDevice>
  <!--采完第一张图，更新数据库仪器型号、仪器型号的名字读取的是该属性-->
  <InstrumentModel>
  </InstrumentModel>
  <!--本诊室的编号-->
  <TheClinic>
  </TheClinic>
</configuration>