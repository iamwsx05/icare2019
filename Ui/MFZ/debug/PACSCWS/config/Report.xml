<?xml version="1.0" encoding="utf-8"?>
<Report>
  <!--对于内窥镜支气管镜，在选择部位画图模式下新增一个手动标识画图模式，  true:选择部位画图模式、false:手动标识画图模式-->
  <BronchusSketchModel>false</BronchusSketchModel>
  <!--保存报告是否弹出MessageBox提示框-->
  <IsShowMessageBox>true</IsShowMessageBox>
  <!--系统模板与自定义模板中检查所见与检查结论文本框是否隐藏true为隐藏-->
  <IsTempLateSeeAndResultVisible>false</IsTempLateSeeAndResultVisible>
  <!--是否开启根据部位加载相关部位    true:开启,false:关闭-->
  <OpenRelevantParts>false</OpenRelevantParts>
  <!--是否开启手术模式-->
  <IsOperation>true</IsOperation>
  <!--是否开启签名自动锁屏功能 true:开启、false:关闭-->
  <OpenAutoLock>true</OpenAutoLock>
  <!--自动锁屏时间间隔  单位:秒-->
  <AutoLockTimeInterval>600</AutoLockTimeInterval>
  <!--配置报告编辑选项页中，哪些选项页显示，比如：系统模板|自定义模板|词库|检查测值-->
  <WhethervisibleTabOptions>系统模板|自定义模板|词库|检查测值</WhethervisibleTabOptions>
  <!--是否开启自动更新-->
  <IsOpenPacsUpdate>false</IsOpenPacsUpdate>
  <!--是否开启 每隔指定时间自动保存报告  true 开启  false 不开启-->
  <OpenAutoSaveReport>false</OpenAutoSaveReport>
  <!--每隔指定时间自动保存报告  单位：毫秒(1000=1秒)-->
  <AutoSaveReportInterval>10000</AutoSaveReportInterval>
  <!--本机器是否允许打印A4纸张报告，如果为false，单击打印，则会由其他电脑打印，这个是适用于远程会诊系统的-->
  <OpenRemotePrintReport>true</OpenRemotePrintReport>
  <!--关于打印报告的时间间隔 1000=1秒-->
  <RemotePrintThreadSpacing>false</RemotePrintThreadSpacing>
  <seeText>检查所见</seeText>
  <resultText>检查结论</resultText>
  <AdviseText>
  </AdviseText>
  <!--报表设计器和病人信息等字段不一致的（大小写不一致可以忽略）-->
  <TemplateNameToPatientInfoName>
    <patient_Uid>patientUserDefine10</patient_Uid>
    <reportDoctor>reportDoctorID</reportDoctor>
    <SendByDepartment>sentByDepartmentID</SendByDepartment>
  </TemplateNameToPatientInfoName>
  <!--报表设计器和数据库等字段不一致的（大小写不一致可以忽略）-->
  <TemplateNameToDataBaseName>
   <sendByDepartment>departmentname </sendByDepartment></TemplateNameToDataBaseName>
  <!--手术模式使用的表（格式  工作站类型1:使用的表1:使用的表2,工作站类型2:使用的表1:使用的表2...）-->
  <OperationTable>PACSCOLPOSCOPY:t_user_Colposcopy_Report:t_SurgicalRecords,PACSNOSETHROATWS:t_user_NOSETHROATWS_Report,PACSCHOLEDOCHOSCOPE:t_user_Choledochoscope_report,FQJWS:T_USER_FQJWS_REPORT</OperationTable>
  <!--是否开启申请审核-->
  <IsOpenApplyVerify>false</IsOpenApplyVerify>
  <!--双击模板是添加还是覆盖(true覆盖，其他情况为添加)-->
  <IsCover>True</IsCover>
  <!--是否最适大小（默认是否有滚动条）-->
  <IsBest>False</IsBest>
  <!--加大多少倍（+为增大，-为缩小）-->
  <MultipleAdd>0</MultipleAdd>
  <!-- 超声工作站是否默认类型 (true是默认类型,false不是默认类型) -->
  <IsUswsDefaultType>false</IsUswsDefaultType>
  <!--Report是否加载图片-->
  <IsLoadImage>False</IsLoadImage>
  <!--Report是否加载测值-->
  <IsLoadMeasured>False</IsLoadMeasured>
  <!--Report是否加载病人信息-->
  <IsLoadPatientInfo>False</IsLoadPatientInfo>
  <!-- 是否开启默认报告医生(false不开启，true开启)-->
  <isDefaultReportDoctorID>true</isDefaultReportDoctorID>
  <!--Report报告宽度比例-->
  <WidthRatio>70</WidthRatio>
  <SeeAndResultHeight>
    <MRSeeHeight>260</MRSeeHeight>
    <MRResultHeight>260</MRResultHeight>
    <MRISeeHeight>260</MRISeeHeight>
    <MRIResultHeight>260</MRIResultHeight>
    <DRSeeHeight>200</DRSeeHeight>
    <DRResultHeight>200</DRResultHeight>
    <DXSeeHeight>200</DXSeeHeight>
    <DXResultHeight>200</DXResultHeight>
    <CRSeeHeight>200</CRSeeHeight>
    <CRResultHeight>200</CRResultHeight>
    <CTSeeHeight>200</CTSeeHeight>
    <CTResultHeight>200</CTResultHeight>
    <USWSSeeHeight>260</USWSSeeHeight>
    <USWSResultHeight>260</USWSResultHeight>
    <彩超SeeHeight>300</彩超SeeHeight>
    <彩超ResultHeight>200</彩超ResultHeight>
    <黑白超SeeHeight>400</黑白超SeeHeight>
    <黑白超ResultHeight>100</黑白超ResultHeight>
    <OTSeeHeight>260</OTSeeHeight>
    <OTResultHeight>260</OTResultHeight>
    <RFSeeHeight>260</RFSeeHeight>
    <RFResultHeight>260</RFResultHeight>
    <胃镜SeeHeight>260</胃镜SeeHeight>
    <胃镜ResultHeight>260</胃镜ResultHeight>
    <肠镜SeeHeight>260</肠镜SeeHeight>
    <肠镜ResultHeight>220</肠镜ResultHeight>
    <十二指肠镜SeeHeight>260</十二指肠镜SeeHeight>
    <十二指肠镜ResultHeight>220</十二指肠镜ResultHeight>
    <支气管镜SeeHeight>260</支气管镜SeeHeight>
    <支气管镜ResultHeight>220</支气管镜ResultHeight>
    <宫腔镜SeeHeight>260</宫腔镜SeeHeight>
    <宫腔镜ResultHeight>220</宫腔镜ResultHeight>
    <关节镜SeeHeight>260</关节镜SeeHeight>
    <关节镜ResultHeight>220</关节镜ResultHeight>
    <阴道镜SeeHeight>260</阴道镜SeeHeight>
    <阴道镜ResultHeight>220</阴道镜ResultHeight>
    <胆道镜SeeHeight>260</胆道镜SeeHeight>
    <胆道镜ResultHeight>220</胆道镜ResultHeight>
    <耳鼻咽喉镜SeeHeight>260</耳鼻咽喉镜SeeHeight>
    <耳鼻咽喉镜ResultHeight>220</耳鼻咽喉镜ResultHeight>
  </SeeAndResultHeight>
  <!--脚注对其方式(left center  right)-->
  <FontNoteOnTheWay>false</FontNoteOnTheWay>
  <AldSet>
    <!--是否开启报告预览截图功能 true:开启-->
    <OpenTheCcreenshot>false</OpenTheCcreenshot>
    <!--根据密码，判断报告样式设置按钮是否启用启用，如果为空，则不需要密码-->
    <LockAldReport>123</LockAldReport>
    <!--报告中图片是否需要下面的标注 true：需要-->
    <WhetherSpecialType>true</WhetherSpecialType>
    <!--报告单中是否图片右边是否显示部位标注图  true:显示  false:不显示-->
    <ShowMarkGraph>true</ShowMarkGraph>
    <!--如果图片右边显示文字，那么设置字体大小 10:10号字体-->
    <PartMarkFontSize>10</PartMarkFontSize>
    <!--检查所见与检查结论字体大小-->
    <SeeAndResultFontSize>16</SeeAndResultFontSize>
    <!--纸质报告单中，检查所见与检查结论字体大小报告控件中的字体大小的字号。如果大2个字号，则+2-->
    <ReportPrintAddFontSize>+0</ReportPrintAddFontSize>
    <!--是否开启  设置工作站一行最多显示的图像数量 开启true-->
    <IsOpenMaxShowImageCount>false</IsOpenMaxShowImageCount>
    <!--设置工作站一行最多显示的图像数量-->
    <ReportMaxShowImageCount>4</ReportMaxShowImageCount>
    <!--设置报告图像数量对应报告单中每行显示的的图像数量格式：两张图一行两张，3张图一行三张 1:1|2:2|3:3|4:2|5:3|6:3|7:4|8:4|9:4-->
    <ImageShowNumber>1:1|2:2|3:3:bottom|4:2|5:3|6:3|7:4|8:4|9:4|10:4|11:4|12:4</ImageShowNumber>
    <!--是否加载医生签名的图片 添加:true-->
    <IsShowSignature>false</IsShowSignature>
    <!---是否加载脚注和急诊脚注true为加载  用|符号分割  true 加载-->
    <WhetherAddFootnote>false|false</WhetherAddFootnote>
    <!--报告行间距-->
    <RowSpacingSize>3</RowSpacingSize>
  </AldSet>
</Report>